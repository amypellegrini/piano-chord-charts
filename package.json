{
  "name": "piano-chord-charts",
  "version": "1.2.0",
  "description": "Piano chord charts SVG library and generator tool.",
  "main": "dist/cli.js",
  "repository": "git@github.com:amypellegrini/piano-chord-charts.git",
  "author": "Amy Pellegrini <amyvpellegrini@gmail.com>",
  "license": "MIT",
  "files": [
    "/dist"
  ],
  "bin": "dist/cli.js",
  "scripts": {
    "build": "tsc && chmod u+x ./dist/cli.js",
    "semantic-release": "semantic-release",
    "prepare": "npm run build",
    "test": "npm run build && jest"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/concat-stream": "^2.0.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^27.4.0",
    "concat-stream": "^2.0.0",
    "cross-spawn": "^7.0.3",
    "jest": "^27.4.7",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "commander": "^9.0.0"
  }
}
